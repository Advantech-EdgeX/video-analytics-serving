{
    "options": {
        "max_running_pipelines": 1
    },
    "pipeline": {
        "name": "object_detection",
        "version": 1
    },
    "request": {
        "source": {
            "type": "uri",
            "uri": "https://github.com/intel-iot-devkit/sample-videos/raw/master/car-detection.mp4"
        },
        "destination": {
            "type": "file",
            "path": "/tmp/object_detection_results.json",
            "format": "json"
        }
    },
    "golden_results": false,
    "check_second_pipeline_queued": true
}