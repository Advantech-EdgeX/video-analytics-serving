{
	"type": "GStreamer",
	"template": ["multifilesrc location=/home/video-analytics-serving/samples/pinwheel.ts loop=True ! concat name=c ! decodebin ! video/x-raw ! videoconvert name=videoconvert",
				" ! gvadetect model-instance-id=inf0 model={models[object_detection][1][network]} model-proc={models[object_detection][1][proc]} name=detection",
				" ! gvametaconvert name=metaconvert ! queue ! gvametapublish name=destination",
				" ! appsink name=appsink"
				],
	"description": "Object Detection Pipeline",
	"pipeline": {
        "name": "stability",
        "version": 1
    },
	"parameters": {
		"type": "object",
		"properties": {
			"device": {
				"element": "detection",
				"type": "string"
			},
			"inference-interval": {
				"element": "detection",
				"type": "integer",
				"minimum": 1,
				"maximum": 4294967295,
				"default": 1
			},
			"cpu-throughput-streams": {
				"element": "detection",
				"type": "integer",
				"minimum": 0,
				"maximum": 4294967295
			},
			"n-threads": {
				"element": "videoconvert",
				"type": "integer",
				"default": 1
			},
			"nireq": {
				"element": "detection",
				"type": "integer",
				"minimum": 1,
				"maximum": 64,
				"default": 2
			}
		}
	}
}
